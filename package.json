{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "NODE_ENV='development' concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "pip3 install -r requirements.txt && next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "start:test": "NODE_ENV=test fastapi-dev"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^2.0.12",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@vercel/analytics": "^1.1.1",
    "autoprefixer": "10.4.14",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "better-react-mathjax": "^2.0.3",
    "concurrently": "^8.0.1",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "geist": "^1.2.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^7.6.8",
    "mongoose-unique-validator": "^4.0.0",
    "next": "^13.5.4",
    "next-auth": "^4.24.5",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.8.0",
    "react-use-websocket": "^4.5.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "cypress": "^13.6.2",
    "dotenv": "^16.3.1"
  }
}
